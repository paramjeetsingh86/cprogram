for (i=0;i<3;i++)
cin>>l[i];
for (i=0;i<3;i++)
cin>>m[i];
for (i=0;i<3;i++)
cin>>n[i];

for (i=0;i<3;i++)
p[i]=l[i]+x[i];   
  
  cout<<"The final matrix after addition is : ";
  for (i=0;i<3;i++)
  cout<< p[i];
